{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Products Collection Request",
    "required": [
      "products"
    ],
    "properties": {
      "products": {
        "$id": "#/properties/products",
        "type": "array",
        "minItems": 1,
        "items": {
          "$id": "#/properties/products/product",
          "type": "object",
          "required": [
            "width",
            "height",
            "length",
            "weight"
          ],
          "properties": {
            "width": {
                "type": "number",
                "exclusiveMinimum": 0
            },
            "height": {
                "type": "number",
                "exclusiveMinimum": 0
            },
            "length": {
                "type": "number",
                "exclusiveMinimum": 0
            },
            "weight": {
                "type": "number",
                "exclusiveMinimum": 0
            }
          }
        }
      }
    }
  }
